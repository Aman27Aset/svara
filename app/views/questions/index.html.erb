<p id="notice"><%= notice %></p>

<h1>Questions</h1>


    <% @questions.each do |question| %>
    <div class="jumbotron" style="margin: 20px auto; max-width: 500px">
        <%= question.body %>
        <%= question.category %>
        <%= question.user.username %>
        <%= link_to 'Show', question %>
        <%= link_to 'Edit', edit_question_path(question) %>
        <%= link_to 'Destroy', question, method: :delete, data: { confirm: 'Are you sure?' } %>
      </div>
    <% end %>

  
  <%= link_to 'New Question', new_question_path, class: 'btn btn-success btn-lg' %>

